#define _GNU_SOURCE
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <unistd.h>
int main() {

  char *line = NULL;
  size_t n = 0;
  printf("Enter programs to run\n");
  getline(&line, &n, stdin);

  pid_t pid = fork();

  if (pid == 0) {
    execl(line, line, (char *)NULL);
  } else {
    printf("Error its the parent process");
  }

  free(line);

  return 0;
}
